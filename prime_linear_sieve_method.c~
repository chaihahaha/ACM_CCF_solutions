#include<stdio.h>
#define MAX_PRI_N 1000000
#define MAX_PRI 1234001
long pri_stk[MAX_PRI_N];
int init_pri_stk(int b){
    //linear sieve method
    int prime[MAX_PRI] = {0};
    int top = 0;
    prime[0] = prime[1] = 1;
    for(int i = 2 ; i < b ; i++){
        if(!prime[i]) pri_stk[top++] = i;
        for(int j = 0 ; j < top && i * pri_stk[j] <= b ; j++){
            prime[i * pri_stk[j]] = 1;
            if(i % pri_stk[j] == 0) break;
        }
    }
    return top;
}
int main() {
    int i = 0;
    int top = init_pri_stk(1000000);
    for(int i = 0; i < top; i++)
        printf("%ld\n",pri_stk[i]);
    return 0;
}
